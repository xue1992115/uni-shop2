<view><view data-event-opts="{{[['tap',[['gotoSearchPage',['$event']]]]]}}" class="search" bindtap="__e"><text>搜索</text></view><swiper indicator-dots="{{true}}" autoplay="{{true}}" interval="{{3000}}" duration="{{1000}}"><block wx:for="{{swiperList}}" wx:for-item="item" wx:for-index="i" wx:key="i"><swiper-item><navigator class="swiper-item" url="{{'/subpkg/good_details/good_details?goods_id='+item.goods_id}}"><image src="{{item.image_src}}"></image></navigator></swiper-item></block></swiper><view class="nav-list"><block wx:for="{{navList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['navHandlerClick',['$0'],[[['navList','',index]]]]]]]}}" class="nav-item" bindtap="__e"><image class="nav-img" src="{{item.image_src}}"></image></view></block></view><view class="floor-list"><block wx:for="{{floorList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="floor-item"><image class="floor-title" src="{{item.floor_title.image_src}}"></image><view class="floor-img-box"><navigator class="left-img-box" url="{{item.product_list[0].url}}"><image style="{{'width:'+(item.product_list[0].image_width+'rpx')+';'}}" src="{{item.product_list[0].image_src}}" mode="widthFix"></image></navigator><view class="right-img-box"><block wx:for="{{item.product_list}}" wx:for-item="item2" wx:for-index="index2" wx:key="index2"><block wx:if="{{index2!==0}}"><navigator class="right-img-item" url="{{item2.url}}"><image class="right-img-src" style="{{'width:'+(item2.image_width+'rpx')+';'}}" src="{{item2.image_src}}" mode="widthFix"></image></navigator></block></block></view></view></view></block></view></view>